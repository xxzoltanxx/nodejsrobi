<!DOCTYPE html>
<html>
<head>
	<title>Najam alata i opreme</title>
  <meta name="description" content="Izvedba gradevinskih radova i najam gradjevinske opreme.">
  <meta name="keywords" content="najam, gradjevina, gradilista, adaptacije">
  <meta name="author" content="FHC d.o.o">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  	<link href="https://fonts.googleapis.com/css?family=Oswald:400,700|Source+Sans+Pro&display=swap" rel="stylesheet">
  	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  	<script
  src="https://code.jquery.com/jquery-2.2.4.min.js"
  integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44="
  crossorigin="anonymous"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

	<link rel="stylesheet" type="text/css" href="homePage.css">
</head>
<body>
	<nav class="navbar navbar-default">
  <div class="container">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>

     <a href="/"><img class="navbar-brand" src = "logo.png"></a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->

    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    	 	<ul class = "nav navbar-nav">
    	 	<li class = "change"><a href="/AboutUs">O nama</a></li>
    		<li class = "change"><a href="/services">Usluge</a></li>
    		<li class = "change"><a href="/contact">Kontakt</a></li>
    	</ul>
    	<ul class = "nav navbar-nav navbar-right">
    		<li class = "change"><a href="/najam" id="currentNajam">Najam Opreme<span class = "glyphicon glyphicon-shopping-cart"></span></a></li>
    	</ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>

<div class = "container opacityNil">
	<form method = "GET" action = "/najam" encType="multipart/form-data">
		  <div class="form-group">
			<label for="exampleInputEmail1">Pretrazi...</label>
			<input type="text" class="form-control" id="exampleInputEmail1" placeholder="Pretrazi..." name="searchTerm">
		  </div>
	</form>
	<div class = "row flexify" >
		
		<% data.forEach(function(entry) { %>
					<div class= "col-xs-12 col-md-4 col-lg-3">
      <div class="panel panel-default">
  <div class="panel-heading"><%= entry.title %></div>
  <div class="panel-body"><img src= <%= entry.picture %> class="panelImage"></div>
  <div class="panel-footer"><div class=center-text>
    <ul>
		<% var lines = entry.lines.split(';'); %>
		<% lines.forEach(function(entry) { %>
			<li><%=entry%></li>
		<%})%>
	</ul>
      <p class="price"><%= entry.price %></p>
	  <form action="/contact" method = "GET">
     <input type="hidden"
            name="oglasTitle"
            value="<%= entry.title %>">
     <button class="btn btn-primary">Posalji upit!</button>
</form>
  </div></div>
</div>
		 </div>
		<% }); %>

	</div>
	
	<div class = "container">
		<div class="row">
			<div class="col-xs-12" id="backgroundCounters">
				<% for (var i = 0; i < pages; ++i) { %>
				<span><a<% if (searchTerm) { %>
				 <% if (i == page) { %> 
					class = "boldedIt" <% } %>
					<% if (i != page) { %>
						 class = "restOfIt" <% } %>
						href = "<%= "najam" + "?page=" + i.toString() + "&searchTerm=" + searchTerm %>" 
					<% } %>
					<% if (!searchTerm) { %>
						<% if (i == page) { %> 
						class = "boldedIt" <% } %>
						 <% if (i != page) { %>
						 class = "restOfIt" <% } %>
						href = "<%= "najam" + "?page=" + i.toString()%>" 
					<% } %>>
					<%= i %></a></span>
				<% } %>
			</div>
		</div>

	</div>
	
  <div class="container" id="footer">
  <div class="row">
  <hr>
    <div class="col-lg-12">
      <div class="col-md-8">  
      </div>
      <div class="col-md-4">
        <p class="muted pull-right">Â© 2020 FHC d.o.o. Sva prava zadrzana</p>
      </div>
    </div>
  </div>
</div>
</div>
</body>
<script type="text/javascript" src="otherPageScript.js"></script>
</html>